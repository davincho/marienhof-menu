{
  "name": "marienhof",
  "version": "0.1.0",
  "repository": "https://github.com/davincho/marienhof-menu",
  "private": true,
  "scripts": {
    "dev": "concurrently 'pnpm css:dev' 'next dev'",
    "build": "pnpm css:build && next build",
    "start": "next start",
    "lint": "next lint",
    "css:dev": "pnpm css:build --watch",
    "css:build": "tailwindcss -i ./styles/global.css -o ./app/global.css"
  },
  "dependencies": {
    "next": "13.0.1",
    "pdf-parse": "^1.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@dvnch/eslint-config": "^3.1.1",
    "@types/node": "18.11.9",
    "@types/pdf-parse": "^1.1.1",
    "@types/react": "18.0.24",
    "@types/react-dom": "18.0.8",
    "autoprefixer": "^10.4.13",
    "concurrently": "^7.5.0",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.1",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.2.1",
    "typescript": "4.8.4"
  }
}
