{
  "name": "marienhof",
  "version": "0.1.0",
  "repository": "https://github.com/davincho/marienhof-menu",
  "private": true,
  "scripts": {
    "dev": "concurrently 'pnpm css:dev' 'next dev'",
    "build": "pnpm css:build && next build",
    "start": "next start --turbo",
    "lint": "next lint",
    "test": "vitest */**/*.test.ts",
    "css:dev": "pnpm css:build --watch",
    "css:build": "tailwindcss -i ./styles/global.css -o ./app/global.css",
    "db:migrate": "dotenv -e .env.development.local -- npx prisma migrate dev",
    "db:deploy": "dotenv -e .env.development.local -- npx prisma migrate deploy",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.6.0",
    "@vercel/analytics": "^1.1.1",
    "@vercel/blob": "^0.15.1",
    "dayjs": "1.11.10",
    "kysely": "^0.26.3",
    "next": "14.0.3",
    "node-html-parser": "^6.1.11",
    "pdf-parse": "^1.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@dvnch/eslint-config": "^4.0.0",
    "@types/node": "20.9.1",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.3.0",
    "eslint": "8.54.0",
    "eslint-config-next": "14.0.3",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "^5.6.0",
    "tailwindcss": "^3.3.5",
    "typescript": "5.2.2",
    "vitest": "^0.34.6"
  }
}
