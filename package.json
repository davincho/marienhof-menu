{
  "name": "marienhof",
  "version": "0.1.0",
  "repository": "https://github.com/davincho/marienhof-menu",
  "private": true,
  "scripts": {
    "dev": "concurrently 'pnpm css:dev' 'next dev'",
    "build": "pnpm css:build && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest */**/*.test.ts",
    "css:dev": "pnpm css:build --watch",
    "css:build": "tailwindcss -i ./styles/global.css -o ./app/global.css"
  },
  "dependencies": {
    "@octokit/core": "^5.0.0",
    "@vercel/analytics": "^1.0.1",
    "next": "13.4.9",
    "node-html-parser": "^6.1.5",
    "pdf-parse": "^1.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "usehooks-ts": "^2.9.1"
  },
  "devDependencies": {
    "@dvnch/eslint-config": "^3.1.1",
    "@types/node": "20.4.2",
    "@types/pdf-parse": "^1.1.1",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "eslint": "8.44.0",
    "eslint-config-next": "13.4.9",
    "postcss": "^8.4.26",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "tailwindcss": "^3.3.3",
    "typescript": "5.1.6",
    "vitest": "^0.33.0"
  }
}
