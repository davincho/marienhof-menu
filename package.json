{
  "name": "marienhof",
  "version": "0.1.0",
  "repository": "https://github.com/davincho/marienhof-menu",
  "private": true,
  "scripts": {
    "dev": "concurrently 'pnpm css:dev' 'next dev'",
    "build": "pnpm css:build && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "vitest */**/*.test.ts",
    "css:dev": "pnpm css:build --watch",
    "css:build": "tailwindcss -i ./styles/global.css -o ./app/global.css",
    "db:migrate": "dotenv -e .env.development.local -- npx prisma migrate dev",
    "db:deploy": "dotenv -e .env.development.local -- npx prisma migrate deploy",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.17.0",
    "@vercel/analytics": "^1.3.1",
    "@vercel/blob": "^0.23.4",
    "babel-plugin-react-compiler": "19.0.0-beta-6fc168f-20241025",
    "dayjs": "1.11.11",
    "kysely": "^0.27.4",
    "next": "15.0.2-canary.10",
    "node-html-parser": "^6.1.13",
    "pdf-parse": "^1.1.1",
    "react": "19.0.0-rc-01172397-20240716",
    "react-dom": "19.0.0-rc-01172397-20240716",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@dvnch/eslint-config": "^4.0.0",
    "@types/node": "20.14.11",
    "@types/pdf-parse": "^1.1.4",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "dotenv-cli": "^7.4.2",
    "eslint": "9.7.0",
    "eslint-config-next": "14.2.5",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "prisma": "^5.17.0",
    "tailwindcss": "^3.4.6",
    "typescript": "5.5.3",
    "vitest": "^2.0.3"
  }
}
